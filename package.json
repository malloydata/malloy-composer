{
  "name": "malloy-composer-demo",
  "description": "Run/Explore/Build Malloy Data Models",
  "version": "0.0.0",
  "config": {
    "malloy_packages": [
      "@malloydata/malloy",
      "@malloydata/db-bigquery",
      "@malloydata/db-duckdb",
      "@malloydata/db-postgres",
      "@malloydata/render"
    ]
  },
  "private": true,
  "main": "./build/main.js",
  "author": "Malloy",
  "license": "GPL-2.0",
  "scripts": {
    "lint": "eslint src scripts",
    "start": "npm run server-start",
    "build": "ts-node scripts/build",
    "clean": "rm -rf ./node_modules",
    "server-build": "ts-node scripts/package-server --skip-package",
    "server-start": "npm run server-build && node ./dist/cli.js --models ./duckdb-wasm",
    "server-package": "ts-node scripts/package-server",
    "build-duckdb-wasm": "ts-node scripts/build-duckdb-wasm",
    "start-duckdb-wasm": "NODE_ENV=development npm run build-duckdb-wasm 9999",
    "watch-duckdb-wasm": "NODE_ENV=development npm run build-duckdb-wasm",
    "malloy-install": "npm --no-fund --no-audit install --save-exact $npm_package_config_malloy_packages",
    "malloy-update": "npm --no-fund --no-audit unlink $npm_package_config_malloy_packages && npm run malloy-install",
    "malloy-link": "npm --no-fund --no-audit link $npm_package_config_malloy_packages",
    "malloy-unlink": "npm --no-fund --no-save --no-audit unlink $npm_package_config_malloy_packages && npm --no-fund --no-audit install --force"
  },
  "dependencies": {
    "@malloydata/db-bigquery": "0.0.7",
    "@malloydata/db-duckdb": "0.0.7",
    "@malloydata/db-postgres": "0.0.7",
    "@malloydata/malloy": "0.0.7",
    "@malloydata/render": "0.0.7",
    "@popperjs/core": "^2.11.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mdast-comment-marker": "^2.1.0",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotkeys": "^2.0.0",
    "react-popper": "^2.2.5",
    "react-query": "^3.34.16",
    "react-router-dom": "^6.4.3",
    "remark-parse": "^10.0.1",
    "shiki": "^0.10.1",
    "styled-components": "^5.3.3",
    "unified": "^10.1.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.18.0",
    "commander": "^9.4.0",
    "concurrently": "^6.2.1",
    "esbuild": "0.14.13",
    "esbuild-plugin-svgr": "^1.0.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "pkg": "^5.8.0",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^10.0.1",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4",
    "wait-on": "^6.0.1"
  },
  "pkg": {
    "scripts": [
      "dist/cli.js"
    ],
    "assets": [
      "dist/**/*",
      "build/**/*"
    ],
    "compress": "GZip"
  }
}
