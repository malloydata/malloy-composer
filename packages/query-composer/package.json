{
  "name": "@malloydata/query-composer",
  "version": "0.0.238",
  "description": "A query composer component for Malloy",
  "main": "dist/cjs/index.cjs",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/esm/index.js"
      },
      "require": {
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json",
    "./example/example_model": "./example/example_model.ts"
  },
  "scripts": {
    "lint": "tsc --noEmit && eslint .",
    "build": "npm run build-bundle && npm run build-types",
    "build-bundle": "vite build --outDir 'dist' --config vite.config.ts",
    "build-types": "tsc --declaration --emitDeclarationOnly",
    "dev": "vite dev --config vite.config.ts",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "test": "jest --runInBand --config=./jest.config.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "moment-js": "^1.1.15",
    "react-popper": "^2.2.5",
    "shiki": "^1.1.6"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@types/lodash": "^4.17.14",
    "@types/moment": "^2.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.0.5",
    "typescript": "5.3.3",
    "vite": "^5.4.12",
    "vite-plugin-svgr": "^4.2.0"
  },
  "peerDependencies": {
    "@malloydata/malloy": ">=0.0.238",
    "@malloydata/render": ">=0.0.238",
    "react": ">=18",
    "react-dom": ">=18",
    "styled-components": "^5.3.3"
  }
}
